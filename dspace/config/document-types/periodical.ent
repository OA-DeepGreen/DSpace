<?xml version="1.0" encoding="UTF-8"?>

<!-- DC.TITLE.TRANSLATED -->
<field>
    <dc-schema>dc</dc-schema>
    <dc-element>title</dc-element>
    <dc-qualifier>translated</dc-qualifier>
    <repeatable>true</repeatable>
    <label>Translated title</label>
    <input-type>onebox</input-type>
    <language value-pairs-name="common_iso_languages">true</language>
    <hint>Enter German translation if your periodical is written in English et vice versa.</hint>
    <required></required>
    <visibility></visibility>
    <type-bind>Periodical</type-bind>
</field>

<!-- DC.TITLE.TRANSLATEDSUBTITLE -->
<field>
    <dc-schema>dc</dc-schema>
    <dc-element>title</dc-element>
    <dc-qualifier>translatedsubtitle</dc-qualifier>
    <repeatable>true</repeatable>
    <label>Translated subtitle</label>
    <input-type>onebox</input-type>
    <language value-pairs-name="common_iso_languages">true</language>
    <hint>Enter German translation of your subtitle if your periodial is written in English et vice versa.</hint>
    <required></required>
    <visibility>workflow</visibility>
    <type-bind>Periodical</type-bind>
</field>

<!-- DC.CONTRIBUTOR.EDITOR -->
<field>
    <dc-schema>dc</dc-schema>
    <dc-element>contributor</dc-element>
    <dc-qualifier>editor</dc-qualifier>
    <repeatable>true</repeatable>
    <label>Editor(s)</label>
    <input-type>name</input-type>
    <hint>Enter the name(s) of the editor(s).</hint>
    <required></required>
	<visibility>workflow</visibility>
    <type-bind>Periodical</type-bind>
</field>

<!-- DC.CONTRIBUTOR.OTHER -->
<field>
    <dc-schema>dc</dc-schema>
    <dc-element>contributor</dc-element>
    <dc-qualifier>other</dc-qualifier>
    <repeatable>true</repeatable>
    <label>Other contributor(s)</label>
    <input-type>name</input-type>
    <hint>Enter the name(s) of other contributor(s).</hint>
    <required></required>
    <type-bind>Periodical</type-bind>
	<visibility>workflow</visibility>
</field>

<!-- DC.CONTRIBUTOR.ORGANISATION -->
<field>
    <dc-schema>dc</dc-schema>
    <dc-element>contributor</dc-element>
    <dc-qualifier>organisation</dc-qualifier>
    <repeatable>true</repeatable>
    <label>Contributing institution</label>
    <input-type>onebox</input-type>
    <hint>Enter the name(s) of the creating/contributing institution.</hint>
    <required></required>
    <type-bind>Periodical</type-bind>
	<visibility>workflow</visibility>
</field>

<!-- DC.DESCRIPTION.ABSTRACT -->
<field>
    <dc-schema>dc</dc-schema>
    <dc-element>description</dc-element>
    <dc-qualifier>abstract</dc-qualifier>
    <repeatable>true</repeatable>
    <label>Abstract</label>
    <input-type>textarea</input-type>
    <language value-pairs-name="common_iso_languages">true</language>
    <hint>Enter a German and an English abstract. Please use "Add more" to enter abstracts in other languages.</hint>
    <required></required>
    <type-bind>Periodical</type-bind>
</field>

<!-- DC.IDENTIFIER -->
<field>
    <dc-schema>dc</dc-schema>
    <dc-element>identifier</dc-element>
    <dc-qualifier></dc-qualifier>
    <repeatable>true</repeatable>
    <label>Identifier(s)</label>
    <input-type value-pairs-name="common_identifiers">qualdrop_value</input-type>
    <hint>If the periodical has any identification numbers or codes associated with it, please enter the types and the actual numbers or codes below.</hint>
    <required></required>
    <type-bind>Periodical</type-bind>
</field>

<!-- DC.PUBLISHER.NAME -->
<field>
    <dc-schema>dc</dc-schema>
    <dc-element>publisher</dc-element>
    <dc-qualifier>name</dc-qualifier>
    <repeatable>false</repeatable>
    <label>Name of the publisher</label>
    <input-type>onebox</input-type>
    <hint>Enter the name of publisher.</hint>
    <required></required>
    <visibility>workflow</visibility>
    <type-bind>Periodical</type-bind>
</field>

<!-- DC.PUBLISHER.PLACE -->
<field>
    <dc-schema>dc</dc-schema>
    <dc-element>publisher</dc-element>
    <dc-qualifier>place</dc-qualifier>
    <repeatable>false</repeatable>
    <label>Place of publisher</label>
    <input-type>onebox</input-type>
    <hint>Enter the place of the publisher.</hint>
    <required></required>
    <visibility>workflow</visibility>
    <type-bind>Periodical</type-bind>
</field>

<!-- DC.DESCRIPTION.SPONSORSHIP-->
<field>
    <dc-schema>dc</dc-schema>
    <dc-element>description</dc-element>
    <dc-qualifier>sponsorship</dc-qualifier>
    <repeatable>true</repeatable>
    <label>Sponsor or funder information</label>
    <input-type>onebox</input-type>
    <hint>Enter the name of the sponsor or funder, and/or project/grant name or ID. Please use the following syntax if applicable: EC/FP7/12345/EU/ProjectName/ProjectAcronym. Please separate multiple entries by semicolon.</hint>
    <required></required>
    <type-bind>Periodical</type-bind>
</field>

<!-- TUB.PUBLISHER.UNIVERSITYORINSTITUTION -->
<field>
    <dc-schema>tub</dc-schema>
    <dc-element>publisher</dc-element>
    <dc-qualifier>universityorinstitution</dc-qualifier>
    <repeatable>false</repeatable>
    <label>Name of the publishing institution</label>
    <input-type value-pairs-name="publishing_institution">dropdown</input-type>
    <hint>The publishing institution is either  "Technische Universität Berlin" or "Universitätsverlag der TU Berlin".</hint>
    <required>You have to choose whether TU Berlin or Universitätsverlag is publishing this item.</required>
     <visibility>workflow</visibility>
    <type-bind>Periodical</type-bind>
</field>

<!-- TUB.ACCESSRIGHTS.DNB -->
<field>
    <dc-schema>tub</dc-schema>
    <dc-element>accessrights</dc-element>
    <dc-qualifier>dnb</dc-qualifier>
    <repeatable>false</repeatable>
    <label>Access rights DNB</label>
    <input-type value-pairs-name="accessrights_dnb">dropdown</input-type>
    <hint>Choose which rights the German National Library (DNB) has to display archived items.</hint>
    <required></required>
    <visibility>workflow</visibility>
    <type-bind>Periodical</type-bind>
</field>

<!-- TUB.ACCESSRIGHTS.openAIRE -->
<field>
    <dc-schema>tub</dc-schema>
    <dc-element>accessrights</dc-element>
    <dc-qualifier>openaire</dc-qualifier>
    <repeatable>false</repeatable>
    <label>Access rights OpenAIRE</label>
    <input-type value-pairs-name="accessrights_openaire">dropdown</input-type>
    <hint>Choose the access rights for this item for OpenAIRE harvesting.</hint>
    <required></required>
    <visibility>workflow</visibility>
    <type-bind>Periodical</type-bind>
</field>
