<!-- DC.CONTRIBUTOR.GRANTOR -->
<field>
    <dc-schema>dc</dc-schema>
    <dc-element>contributor</dc-element>
    <dc-qualifier>grantor</dc-qualifier>
    <repeatable>false</repeatable>
    <label>Organisation</label>
    <!--<input-type value-pairs-name="granting_organisation">dropdown</input-type>-->
    <!--
    PREFILL WITH ITEM TEMPLATE VALUE FOR ALL PUBLICATIONS collections. CHANGE IF NECESSARY!
    -->
    <input-type>onebox</input-type>
    <hint>Choose the name of the granting institution.</hint>
    <required>You have to choose the name of the granting institution.</required>
    <type-bind>Bachelor Thesis</type-bind>
</field>

<!-- DC.DATE.ACCEPTED -->
<field>
    <dc-schema>dc</dc-schema>
    <dc-element>date</dc-element>
    <dc-qualifier>accepted</dc-qualifier>
    <repeatable>false</repeatable>
    <label>Date of final exam</label>
    <input-type>date</input-type>
    <hint>Enter the date of the final exam.</hint>
    <required></required>
    <type-bind>Bachelor Thesis</type-bind>
</field>

<!-- DC.DESCRIPTION.ABSTRACT -->
<field>
    <dc-schema>dc</dc-schema>
    <dc-element>description</dc-element>
    <dc-qualifier>abstract</dc-qualifier>
    <repeatable>true</repeatable>
    <label>Abstract</label>
    <input-type>textarea</input-type>
    <hint>Enter a German and an English abstract. Please use "Add more" to enter abstracts in other languages.</hint>
    <required>You have to enter an abstract.</required>
    <type-bind>Bachelor Thesis</type-bind>
</field>

<!-- DC.SUBJECT.DDC -->
<field>
    <dc-schema>dc</dc-schema>
    <dc-element>subject</dc-element>
    <dc-qualifier>ddc</dc-qualifier>
    <repeatable>true</repeatable>
    <label>DDC Class</label>
    <input-type>onebox</input-type>
    <hint>The Dewey Decimal Classification (DDC) is the international most widely used classification. Enter appropriate DDC Class(es) below.</hint>
    <required>You have to enter a DDC Class.</required>
    <vocabulary>ddc</vocabulary>
    <type-bind>Bachelor Thesis</type-bind>
</field>

<!-- DC.IDENTIFIER -->
<field>
    <dc-schema>dc</dc-schema>
    <dc-element>identifier</dc-element>
    <dc-qualifier></dc-qualifier>
    <repeatable>true</repeatable>
    <label>Identifier(s)</label>
    <input-type value-pairs-name="common_identifiers">qualdrop_value</input-type>
    <hint>If the Bachelor thesis has any identification numbers or codes associated with it, please enter the types and the actual numbers or codes below.</hint>
    <required></required>
    <type-bind>Bachelor Thesis</type-bind>
</field>

<!-- DC.DESCRIPTION.SPONSORSHIP-->
<field>
    <dc-schema>dc</dc-schema>
    <dc-element>description</dc-element>
     <dc-qualifier>sponsorship</dc-qualifier>
    <repeatable>true</repeatable>
    <label>Sponsor or funder information</label>
    <input-type>onebox</input-type>
    <hint>Enter the name of the sponsor or funder, and/or project/grant name or ID. Please separate multiple entries by semicolon.</hint>
    <required></required>
    <type-bind>Bachelor Thesis</type-bind>
</field>

<!-- DC.TYPE.VERSION -->
<field>
    <dc-schema>dc</dc-schema>
    <dc-element>type</dc-element>
    <dc-qualifier>version</dc-qualifier>
    <repeatable>false</repeatable>
    <label>Version</label>
    <input-type value-pairs-name="type_versions">dropdown</input-type>
    <hint>Choose the type of version. For type "Bachelor thesis" choose either acceptedVersion, publishedVersion for a thesis published by a publishing house or updatedVersion for any version of the thesis that has been updated since its first publication.</hint>
    <required>You have to choose the type of the published version.</required>
    <visibility>workflow</visibility>
	<type-bind>Bachelor thesis</type-bind>
</field>

